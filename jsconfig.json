{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./*"
      ],
      "@/components": [
        "app/components"
      ],
      "@/components/*": [
        "app/components/*"
      ],
      "@/containers": [
        "app/containers"
      ],
