{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/components": [
        "app/components"
      ],
      "@/components/*": [
        "app/components/*"
      ],
      "@/containers": [
        "app/containers"
      ],
      "@/containers/*": [
        "app/containers/*"
      ],
      "@/helpers/*": [
        "app/helpers/*"
      ],
      "@/sass/*": [
        "app/sass/*"
      ],
    }
  }
}
